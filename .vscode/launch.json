{
  "version": "1.0.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest: current file",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "args": ["${fileBasenameNoExtension}", "--config", "jest.config.json"],
      "console": "integratedTerminal"
    }
  ]
}
